{
    "log": {
        "loglevel": "debug"
    },
    "inbounds": [
        {
            "port": 53,
            "tag": "dns_in",
            "protocol": "dokodemo-door",
            "settings": {
                "address": "FAKEIP",
                "port": 53,
                "network": "udp",
                "timeout": 0,
                "followRedirect": false,
                "userLevel": 0
            }
        }
    ],
    "dns": {
        "servers": [
            { "address": "223.5.5.5", "port": 53, "domains": ["geosite:cn"], "expectIPs": ["geoip:cn"] }
        ],
        "queryStrategy": "UseIPv4",
        "disableCache": true,
        "disableFallback": true
    },
    "routing": {
        "rules": [
            {
                "type": "field",
                "inboundTag": ["dns_in"],
                "outboundTag": "direct"
            },
            {
                "type": "field",
                "inboundTag": ["dns_out"],
                "outboundTag": "direct"
            }
        ]
    },
    "policy": {},
    "outbounds": [
        {
            "tag": "forward",
            "protocol": "dns" 
        },
        {
            "tag":"direct",
            "protocol": "freedom"
        } 
    ]
}