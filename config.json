{
    "inbounds": [
        {
            "port": 53,
            "tag": "dns_in",
            "protocol": "dokodemo-door",
            "setttings": {
                "address": "LOCALDNS",
                "port": 53,
                "network": "udp",
                "timeout": 0,
                "followRedirect": false
            }
        }
    ],
    "dns": {
        "servers": [
            { "address": "FAKEDNS", "port": 53 },
            { "address": "ISPDNS", "port": 53, "domains": ["geosite:cn"], "expectIPs": ["geoip:cn"] }
        ],
        "queryStrategy": "UseIPv4",
        "disableCache": true,
        "disableFallback": true,
        "tag": "dns"
    },
    "routing": {
        "rules": [
            {
                "type": "field",
                "inboundTags": ["dns_in"],
                "outboundTag": "forward"
            }
        ]
    },
    "policy": {},
    "outbounds": [
        {
            "tag": "forward",
            "protocol": "dns" 
        } 
    ]
}
