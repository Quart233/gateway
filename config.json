{
    "log": {
        "loglevel": "debug"
    },
    "inbounds": [
        {
            "port": 53,
            "tag": "dns_in",
            "protocol": "dokodemo-door",
            "settings": {
                "address": "127.0.0.1",
                "port": 9494,
                "network": "tcp,udp",
                "timeout": 0,
                "followRedirect": false,
                "userLevel": 0
            }
        }
    ],
    "dns": {
        "servers": [
            { "address": "192.168.24.1", "port": 53, "domains": ["geosite:cn"], "expectIPs": ["geoip:cn"] }
        ],
        "queryStrategy": "USE_IP4",
        "cacheStrategy": "CacheDisabled",
        "fallbackStrategy": "Disabled"
    },
    "routing": {
        "rules": [
            {
                "type": "field",
                "inboundTag": ["dns_in"],
                "outboundTag": "forward"
            }
        ]
    },
    "policy": {},
    "outbounds": [
        {
            "tag": "forward",
            "protocol": "dns" 
        }
    ]
}

